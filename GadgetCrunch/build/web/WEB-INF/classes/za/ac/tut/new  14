package za.ac.tut.gadget.gadgetextend;
import za.ac.tut.gadget.Gadget;

public class GadgetExtend
{
	private int id;
	private Gadget gadget;
	
	public GadgetExtend()
	{
	
	}
	
	public GadgetExtend(int id, Gadget gadget)
	{
		this.id = id;
		this.gadget = gadget;
	}
	
	public void setID(int id)
	{
		this.id = id;
	}
	
	public int getID()
	{
		return this.id;
	}
	
	public void setGadget(Gadget gadget)
	{
		this.gadget = gadget;
	}
	
	public Gadget getGadget()
	{
		return this.gadget;
	}
	
}